list := []number

x := 1
while x <= 1000 {
    list.append(x)
    assertEq(list.reduce(func(tot: number, a: number) number {
        return tot + a
    }, 0), x*(x + 1)/2)

    x = x + 1
}

func sum(a: number, b: number) number {
    return a + b
}

list = []number
x = 1
while x <= 100 {
    list.append(2*x - 1)
    assertEq(list.reduce(sum, 0), x*x)

    x = x + 1
}

assertEq([1, 2] + [3], [1, 2, 3])
assertEq(["Eny", "meanie"] + ["minie", "moe"], ["Eny", "meanie", "minie", "moe"])
